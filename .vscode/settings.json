{
	"python.formatting.provider": "yapf",
	"python.pythonPath": ".venv\\Scripts\\python.exe",
	"python.linting.pylintEnabled": false,
	"python.linting.mypyEnabled": true,
	"files.exclude": {
		"**/.git": true,
		"**/.svn": true,
		"**/.hg": true,
		"**/CVS": true,
		"**/.DS_Store": true,
		"**/.venv": true,
		"**/__pycache__": true,
		"**/.mypy_cache": true,
		"**/.pytest_cache": true,
		"**/.coverage": true,
		"**/build": true,
		"**/dist": true
	}
}
